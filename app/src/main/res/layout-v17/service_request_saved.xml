                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:layout_gravity="left"
                                    android:gravity="left"
                                    android:layout_weight="1"/>
                            </LinearLayout>
                    <LinearLayout
