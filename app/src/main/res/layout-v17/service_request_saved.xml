                                    android:id="@+id/txtTitleLearning"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:text="@string/Learning"
                                    android:layout_gravity="right"
                                    android:textSize="16sp"
