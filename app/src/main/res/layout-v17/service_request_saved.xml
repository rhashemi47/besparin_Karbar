                                android:id="@+id/txtTitlePeriodAndEmergency"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="#000000"
                                android:text="@string/PeriodAndEmergency"
                                android:layout_gravity="right"
                                android:textSize="16sp"
                                android:layout_weight="1"/>
                            <TextView
                                android:id="@+id/txtPeriodAndEmergency"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="#000000"
                                android:layout_gravity="left"
                                android:gravity="left"
                                android:layout_weight="1"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/LinearCountHamyar"
                            android:visibility="gone">
                            <TextView
                                android:id="@+id/txtTitleCountHamyar"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="#000000"
                                android:text="@string/CountHamyar"
                                android:layout_gravity="right"
                                android:textSize="16sp"
                                android:layout_weight="1"/>
                            <TextView
